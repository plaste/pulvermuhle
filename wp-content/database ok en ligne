-- --------------------------------------------------------

--
-- Structure de la table 'fp_terms'
--

CREATE TABLE IF NOT EXISTS fp_terms (
  term_id bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `name` varchar(200) NOT NULL DEFAULT '',
  slug varchar(200) NOT NULL DEFAULT '',
  term_group bigint(10) NOT NULL DEFAULT '0',
  term_order int(4) DEFAULT '0',
  PRIMARY KEY (term_id),
  KEY slug (slug(191)),
  KEY `name` (`name`(191))
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 AUTO_INCREMENT=9 ;

--
-- Contenu de la table 'fp_terms'
--

INSERT INTO fp_terms (term_id, name, slug, term_group, term_order) VALUES
(1, 'Non classé', 'non-classe', 0, 3),
(2, 'Unkategorisiert', 'unkategorisiert', 0, 4),
(3, 'Menu principal', 'menu-principal', 0, 0),
(4, 'Actualités', 'actualites', 0, 2),
(5, 'Actualités', 'actualites-de', 0, 1),
(6, 'Menu de pied de page', 'menu-de-pied-de-page', 0, 0),
(7, 'Menu principal - De', 'menu-principal-de', 0, 0),
(8, 'Menu de pied de page - De', 'menu-de-pied-de-page-de', 0, 0);

-- --------------------------------------------------------

--
-- Structure de la table 'fp_term_relationships'
--

CREATE TABLE IF NOT EXISTS fp_term_relationships (
  object_id bigint(20) unsigned NOT NULL DEFAULT '0',
  term_taxonomy_id bigint(20) unsigned NOT NULL DEFAULT '0',
  term_order int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (object_id,term_taxonomy_id),
  KEY term_taxonomy_id (term_taxonomy_id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

--
-- Contenu de la table 'fp_term_relationships'
--

