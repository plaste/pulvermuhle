-- --------------------------------------------------------

--
-- Structure de la table 'fp_icl_translation_batches'
--

CREATE TABLE IF NOT EXISTS fp_icl_translation_batches (
  id int(11) NOT NULL AUTO_INCREMENT,
  batch_name text NOT NULL,
  tp_id int(11) DEFAULT NULL,
  ts_url text,
  last_update datetime DEFAULT NULL,
  PRIMARY KEY (id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Structure de la table 'fp_icl_translation_status'
--

CREATE TABLE IF NOT EXISTS fp_icl_translation_status (
  rid bigint(20) NOT NULL AUTO_INCREMENT,
  translation_id bigint(20) NOT NULL,
  `status` tinyint(4) NOT NULL,
  translator_id bigint(20) NOT NULL,
  needs_update tinyint(4) NOT NULL,
  md5 varchar(32) NOT NULL,
  translation_service varchar(16) NOT NULL,
  batch_id int(11) NOT NULL DEFAULT '0',
  translation_package text NOT NULL,
  `timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  links_fixed tinyint(4) NOT NULL DEFAULT '0',
  _prevstate longtext,
  PRIMARY KEY (rid),
  UNIQUE KEY translation_id (translation_id)
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 AUTO_INCREMENT=39 ;

